{
  "system": {
    "version": "3.0",
    "voice_enabled": true,
    "stream_mode": true,
    "debug": false,
    "log_level": "WARNING"
  },
  "api": {
    "api_key": "your-api-key-here",
    "base_url": "https://api.deepseek.com/v1",
    "model": "deepseek-chat",
    "temperature": 0.7,
    "max_tokens": 2000,
    "max_history_rounds": 10
  },
  "api_server": {
    "enabled": true,
    "host": "127.0.0.1",
    "port": 8000,
    "auto_start": true,
    "docs_enabled": true
  },
  "grag": {
    "enabled": true,
    "auto_extract": true,
    "context_length": 5,
    "similarity_threshold": 0.6,
    "neo4j_uri": "neo4j://127.0.0.1:7687",
    "neo4j_user": "neo4j",
    "neo4j_password": "your-neo4j-password",
    "neo4j_database": "neo4j"
  },
  "handoff": {
    "max_loop_stream": 5,
    "max_loop_non_stream": 5,
    "show_output": false
  },
  "browser": {
    "playwright_headless": false
  },
  "tts": {
    "api_key": "your_tts_api_key_here",
    "port": 5050,
    "default_voice": "en-US-AvaNeural",
    "default_format": "mp3",
    "default_speed": 1.0,
    "default_language": "en-US",
    "remove_filter": false,
    "expand_api": true,
    "require_api_key": true
  },
  "weather_api_key": "your-weather-api-key",
  "mqtt": {
    "enabled": false,
    "broker": "localhost",
    "port": 1883,
    "topic": "/test/topic",
    "client_id": "naga_mqtt_client",
    "username": "",
    "password": "",
    "keepalive": 60,
    "qos": 1
  },
  "ui": {
    "user_name": "用户",
    "bg_alpha": 0.5,
    "window_bg_alpha": 110,
    "mac_btn_size": 36,
    "mac_btn_margin": 16,
    "mac_btn_gap": 12,
    "animation_duration": 600
  }
}
